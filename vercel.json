{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/cart-basic",
      "status": 302,
      "headers": { "Location": "/" }
    },
    {
      "src": "/cart",
      "status": 302,
      "headers": { "Location": "/" }
    },
    {
      "src": "/auth/signup",
      "status": 302,
      "headers": { "Location": "/" }
    },
    {
      "src": "/auth/login",
      "status": 302,
      "headers": { "Location": "/" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
}